# AWS Configuration for DynamoDB
# Required for accessing the Wordbook DynamoDB table

# AWS Region where your DynamoDB table is hosted
AWS_REGION=us-east-1

# AWS Credentials (use IAM user with DynamoDB access)
AWS_ACCESS_KEY_ID=your_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_secret_access_key_here

# API Configuration
# Base URL for API endpoints (used in client-side code)
PUBLIC_API_BASE_URL=http://localhost:5173
